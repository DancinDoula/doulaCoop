{
  "compilerOptions": {
    "module": "commonjs",
    "allowJs": true,
    "outDir": "src",
    "target": "es5",
    "noImplicitAny": false,
    "sourceMap": false,
    "noUnusedLocals": true,
    "allowUnreachableCode": false,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "lib": ["es2015"]
  },
  "include": ["./src/index.ts"],
  "exclude": [""]
}
