{
  "compilerOptions": {
    "module": "commonjs",
    "allowJs": true,
    "outDir": "src/js",
    "target": "es5",
    "noImplicitAny": false,
    "sourceMap": false,
    "allowUnreachableCode": false,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "lib": ["es2015", "es2016", "dom"]
  },
  "include": ["./src/index.ts"],
  "exclude": [""]
}
